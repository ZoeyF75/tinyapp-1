<script>
  function initFingerprintJS() {
    FingerprintJS.load()
    .then(fp => fp.get())
    .then(result => fetch(`/urls/<%-`${shortUrl}`%>/${result.visitorId}/track`))
    .then((result)=>{
      window.location.replace("<%-`${longUrl}`%>")
    })
    .catch((result)=>{
      window.location.replace("<%-`${longUrl}`%>")
    })}
</script>
<script
  async
  src="/fp.min.js"
  onload="initFingerprintJS()"
></script>
